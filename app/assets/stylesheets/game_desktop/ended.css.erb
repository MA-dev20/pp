#gd_head_ended {
    position: relative;
    top: 20px;
    height: 60px;
    line-height: 60px;
    left: 0;
    width: 100%;
    text-align: center;
    font-size: 8.5vh;
    font-weight: bold;
    color: white;
}
#gd_ended {
    position: absolute;
    bottom: 0;
    left: 0;
    height: 67.265vh;
    width: 100%;
}
#gd_ended .rate_1,
#gd_ended .rate_2,
#gd_ended .rate_3 {
    position: absolute;
    width: 250px;
    height: 60vh;
    bottom: 0;
    animation-name: move-in-bottom;
    animation-duration: 4s;
    animation-iteration-count: 1;
}
#gd_ended .rate_1 .line,
#gd_ended .rate_2 .line,
#gd_ended .rate_3 .line{
    position: absolute;
    bottom: 0;
    left: 100px;
    width: 80px;
    height: 50vh;
    box-shadow: 0 1rem 3rem 0 rgba(0,0,0,0.5);
}
#gd_ended .rate_1 .rating,
#gd_ended .rate_2 .rating,
#gd_ended .rate_3 .rating{
    position: absolute;
    left:0px;
}
#gd_ended .rating .circle {
    position: absolute;
    top: 0;
    left: 0;
    height: 280px;
    width: 280px;
    z-index: 10;
    border-radius: 50%;
    line-height: 140px;
    text-align: center;
    color: white;
    box-shadow: 0 1rem 3rem 0 rgba(0,0,0,0.5);
}
