#gd_head_ended {
    position: absolute;
    bottom: calc( 50vh + 300px);
    height: 60px;
    line-height: 60px;
    left: 0;
    width: 100%;
    text-align: center;
    font-size: 58px;
    font-weight: bold;
    color: white;
}
#gd_ended {
    position: absolute;
    bottom: 0;
    left: 0;
    height: 600px;
    width: 100%;
}
#gd_ended .rate_1,
#gd_ended .rate_2,
#gd_ended .rate_3 {
    position: absolute;
    width: 250px;
    height: 60vh;
    bottom: 0;
    animation-name: move-in-bottom;
    animation-duration: 4s;
    animation-iteration-count: 1;
}
#gd_ended .rate_1 {
    left: calc(50vw - 125px);
}
#gd_ended .rate_2 {
    left: calc(30vw - 125px);
}
#gd_ended .rate_3 {
    left: calc(70vw - 125px);
}
#gd_ended .rate_1 .line,
#gd_ended .rate_2 .line,
#gd_ended .rate_3 .line{
    position: absolute;
    bottom: 0;
    left: 80px;
    width: 90px;
    height: 50vh;
    border-left: 5px solid #d2f8ef;
    border-right: 5px solid #d2f8ef;
    box-shadow: 0 1rem 3rem 0 rgba(0,0,0,0.5);
}
#gd_ended .rate_1 .line {
    height: 50vh;
}
#gd_ended .rate_2 .line {
    height: 40vh;
}
#gd_ended .rate_3 .line {
    height: 30vh;
}

#gd_ended .rate_1 .rating,
#gd_ended .rate_2 .rating,
#gd_ended .rate_3 .rating{
    position: absolute;
    left:0px;
    height: 250px;
    width: 250px;
}
#gd_ended .rate_1 .rating {
    bottom: 45vh;
}
#gd_ended .rate_2 .rating {
    bottom: 35vh;
}
#gd_ended .rate_3 .rating {
    bottom: 25vh;
}
#gd_ended .rating .circle {
    position: absolute;
    top: 50px;
    left: 35px;
    height: 180px;
    width: 180px;
    background-color: #00e38b;
    z-index: 10;
    border-radius: 50%;
    line-height: 140px;
    text-align: center;
    color: white;
    box-shadow: 0 1rem 3rem 0 rgba(0,0,0,0.5);
}
#gd_ended .rating .circle img {
    position: absolute;
    top: 2px;
    left: 2px;
    height: 176px;
    width: 176px;
    opacity: 1;
    border-radius: 50%;
}
#gd_ended .rating .circle .avatar {
    position: absolute;
    display: none;
    top: 10px;
    left: 10px;
    height: 160px;
    width: 160px;
    opacity: 1;
}
#gd_ended .rating .pol,
#gd_ended .rating .rate_ges {
    position: absolute;
    display: none;
    bottom: 0;
    right: 0;
    height: 100px;
    width: 100px;
    opacity: 0.8;
    z-index: 100;
}

#gd_ended .rating .rate_ges {
    line-height: 100px;
    opacity: 1;
    font-size: 40px;
    color: white;
    text-align: center;
}
#gd_ended .rating .crown1,
#gd_ended .rating .crown2,
#gd_ended .rating .crown3 {
    display: none;
    position: absolute;
    z-index: 0;
    animation: buzz-out;
    animation-duration: 1s;
}
#gd_ended .rating .crown1 {
    top: 10px;
    right: 7px;
}
#gd_ended .rating .crown2 {
    top: -5px;
    left: 50px;
}
#gd_ended .rating .crown3 {
    top: 0;
    right: 30px;
}

@keyframes move-in-bottom {
    from {
        transform: translateY(3000px);
    }
}
@-webkit-keyframes buzz-out {
  10% {
    -webkit-transform: translateX(3px) rotate(2deg);
    transform: translateX(3px) rotate(2deg);
  }
  20% {
    -webkit-transform: translateX(-3px) rotate(-2deg);
    transform: translateX(-3px) rotate(-2deg);
  }
  30% {
    -webkit-transform: translateX(3px) rotate(2deg);
    transform: translateX(3px) rotate(2deg);
  }
  40% {
    -webkit-transform: translateX(-3px) rotate(-2deg);
    transform: translateX(-3px) rotate(-2deg);
  }
  50% {
    -webkit-transform: translateX(2px) rotate(1deg);
    transform: translateX(2px) rotate(1deg);
  }
  60% {
    -webkit-transform: translateX(-2px) rotate(-1deg);
    transform: translateX(-2px) rotate(-1deg);
  }
  70% {
    -webkit-transform: translateX(2px) rotate(1deg);
    transform: translateX(2px) rotate(1deg);
  }
  80% {
    -webkit-transform: translateX(-2px) rotate(-1deg);
    transform: translateX(-2px) rotate(-1deg);
  }
  90% {
    -webkit-transform: translateX(1px) rotate(0);
    transform: translateX(1px) rotate(0);
  }
  100% {
    -webkit-transform: translateX(-1px) rotate(0);
    transform: translateX(-1px) rotate(0);
  }
}

#confetti {
    left: 0;
    position: fixed;
    right: 0;
    top: -160px;
    z-index: 10000;
}
#confetti .ball {
    animation-iteration-count :infinite !important;
    cursor: default;
    display: inline-block;
    height: 15px;
    user-select: none; 
    width: 10px;
}

 
@keyframes falling-left {
    0% { opacity: 1;}
    10% { opacity: 0;}
    20% { opacity: 1;}
    30% { opacity: 0;}
    40% { opacity: 1;}
    50% { opacity: 0;}
    60% { opacity: 1;}
    70% { opacity: 0;}
    80% { opacity: 1;}
    90% { opacity: 0;}
    100% {transform: translate3d(0,120vh,0) rotate3d(0, 1, 2, 180deg); opacity: 1;}
}
 
#confetti .ball:nth-child(2n) {
    animation: falling-left linear ;
}
 
@keyframes falling-right {
    0% {opacity: 1;}
    10% { opacity: 0;}
    20% { opacity: 1;}
    30% { opacity: 0;}
    40% { opacity: 1;}
    50% { opacity: 0;}
    60% { opacity: 1;}
    70% { opacity: 0;}
    80% { opacity: 1;}
    90% { opacity: 0;}
    100% {transform: translate3d(0,120vh,0) rotate3d(0, 1, 2, 180deg); opacity: 0;}  
}
 
#confetti .ball:nth-of-type(2n+1) {
    animation: falling-right linear;
}